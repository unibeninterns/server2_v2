{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "moduleResolution": "node",
      "typeRoots": ["./node_modules/@types"],
      "types": ["node", "express"]
    },
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
    "node_modules",
    "dist"
  ]
  }